        "163914199120393d3e-0": {
          "type": "image",
          "settings": {
            "featured_image": "shopify:\/\/shop_images\/website-01.jpg",
            "job_title": "Subscribe To Your Box",
            "job_position": "Monthly curation of premium beauty products across cosmetics, skincare, haircare and fragrance. Minimum of five luxury products per month. A monthly copy of our Cohorted Beauty Box Booklet Mini Magazine. Reliable dispatch straight to your door in bespoke 100% recyclable packaging. Exclusive offers and discounts available to Cohorted subscribers.",
            "linktext": "",
            "link": "",
            "sectionOrder": "orderOne"
          }
        },
        "8e0df266-bde3-4d44-8193-0506625239d4": {
          "type": "image",
          "settings": {
            "featured_image": "shopify:\/\/shop_images\/website-02.jpg",
            "job_title": "Order & Dispatch",
            "job_position": "The Cohorted Beauty Box subscription works one month in advance. This means that you will receive your first beauty box between the 5th and the 15th of the following month. For example, if you sign up in January your first box will be the February edition. All of our boxes are hand-wrapped in 100% sustainable packaging. For your peace of mind, we send all of our boxes using 48 hour recorded delivery.",
            "linktext": "",
            "link": "",
            "sectionOrder": "orderOne"
          }
        },
        "fc50b8e7-0dd6-406f-8db2-a07c85daadce": {
          "type": "image",
          "settings": {
            "featured_image": "shopify:\/\/shop_images\/website-03.jpg",
            "job_title": "Billing & Cancellation",
            "job_position": "Payment for your first Beauty Box will be taken when you sign up. You will then be billed on a rolling monthly basis between the 1st and 3rd of each month until a request for cancellation is submitted. Our subscription is completely flexible allowing you to cancel or pause your subscription at any time. You can simply process the request from your Subscription Dashboard or send a request to our customer service team before the last day of each month.",
            "linktext": "",
            "link": "",
            "sectionOrder": "orderOne"
          }
        }
      },
