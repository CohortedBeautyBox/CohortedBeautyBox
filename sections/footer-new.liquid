                      {% assign footerCol3 = settings.footer_column3_linklist %}
                      {% for link in linklists[footerCol3].links %}
                      <li>{{ link.title | link_to: link.url }}</li>
                      {% endfor %}
                    </ul>
                  </div>

                </div>
                <div class="col-md-3 col-sm-3">
                  <div class="widget-heading clearfix">
                    <h3>
                      <span>{{ settings.footer_column4_title }}</span>
                      <span class="line-ft"></span>
                    </h3>
                  </div>
                  <div class="widget-text">
                    {{ settings.shop_contact_infos }}	
                    <div class="socail_title">FOLLOW US</div>
                    <ul class="footer icon">
                      {% if settings.footer_facebook_link != '' %}
                      <li><a href="{{settings.footer_facebook_link}}"><i class="fa fa-facebook"></i></a></li>
                      {% endif %}
                      {% if settings.footer_twitter_link != '' %}
                      <li><a href="{{settings.footer_twitter_link}}"><i class="fa fa-twitter"></i></a></li>
                      {% endif %}
                      {% if settings.footer_pinterest_link != '' %}
                      <li><a href="{{settings.footer_pinterest_link}}"><i class="fa fa-pinterest-p"></i></a></li>
                      {% endif %}
                      {% if settings.footer_instagram_link != '' %}
                      <li><a href="{{settings.footer_instagram_link}}"><i class="fa fa-instagram"></i></a></li>
                      {% endif %}
                      {% if settings.footer_lindin_link != '' %}
                      <li><a href="{{settings.footer_lindin_link}}"><i class="fa fa-linkedin-square"></i></a></li>
                      {% endif %}
                      {% if settings.footer_googleplus_link != '' %}
                      <li><a href="{{settings.footer_googleplus_link}}"><i class="fa fa-google-plus"></i></a></li>
                      {% endif %}
                      
                    </ul>			
                  </div>	
                </div>										
              </div>
