            </div>
          </div>
          <div id="footer_c">
            <div class="container">
              <!-- Information -->
              <div class="row">
                <div class="col-md-3 col-sm-3">
                  <div class="widget-heading clearfix">
                    <h3>
                      <span>{{ settings.footer_column1_title }}</span>
                      <span class="line-ft"></span>
                    </h3>
                  </div>
                  <div class="widget-text">
                    
                    
                    
                    <ul>
                      {% assign footerCol1 = settings.footer_column1_linklist %}
                      {% for link in linklists[footerCol1].links %}
                      <li>{{ link.title | link_to: link.url }}</li>
                      {% endfor %}
                    </ul>
                  </div>

                </div>
                <div class="col-md-3 col-sm-3">
                  <div class="widget-heading clearfix">
                    <h3>
                      <span>{{ settings.footer_column2_title }}</span>
                      <span class="line-ft"></span>
                    </h3>
                  </div>
                  <div class="widget-text">
                    <ul>
                      {% assign footerCol2 = settings.footer_column2_linklist %}
                      {% for link in linklists[footerCol2].links %}
                      <li>{{ link.title | link_to: link.url }}</li>
                      {% endfor %}
                    </ul>
                  </div>

                </div>
                <div class="col-md-3 col-sm-3">
                  <div class="widget-heading clearfix">
                    <h3>
                      <span>{{ settings.footer_column3_title }}</span>
                      <span class="line-ft"></span>
                    </h3>
